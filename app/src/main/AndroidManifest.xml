<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.amanpandey.red_bank">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Red_Bank">
        <activity android:name=".Activites.SearchResult"/>

        <activity android:name=".Activites.MakeRequest" />
        <activity android:name=".Activites.Search" />
        <activity android:name=".Activites.Login" />
        <activity android:name=".Activites.Register" />
        <activity android:name=".Activites.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activites.MainActivity" />
    </application>

</manifest>